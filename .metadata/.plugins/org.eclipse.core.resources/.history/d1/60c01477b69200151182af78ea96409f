package org.ics.llc.languageRelated;

public enum LanguageType {
	CSharp("c#",0),
	CPlusPlus,
	C,
	CSS,
	HTML,
	Java,
	JavaScript,
	ObjectiveC,
	PHP,
	Perl,
	Python,
	Ruby,
	SQL;
	
	private String _name;
	private int _id;
	
	LanguageType(String name, int id)
	{
		_name = name;
		_id = id;
	}
	
	public String toString()
	{
		return _name;
	}
}
